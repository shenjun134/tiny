<div id="dialog-image-temp" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="dialog-image-temp" aria-hidden="true">
  <div class="modal-dialog" role="document" style="width: 100%; height: calc(100% -  60px); padding: 40px;">
    <div class="modal-content" style="height: 100%;">
      <div class="modal-body" style="height: 100%;">
        ...
      </div>
    </div>
  </div>
</div>

<div id="dialog-image-cover" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="dialog-image-cover" aria-hidden="true">
  <div class="modal-dialog" role="document" style="width: 100%; height: calc(100% -  60px); padding: 40px;">
    <div class="modal-content" style="height: 100%;">
      <div class="modal-body" style="height: 100%;">
        ...
      </div>
    </div>
  </div>
</div>

<div id="dialog-image-signature" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="dialog-image-signature" aria-hidden="true">
  <div class="modal-dialog" role="document" style="width: 100%; height: calc(100% -  60px); padding: 40px;">
    <div class="modal-content" style="height: 100%;">
      <div class="modal-body" style="height: 100%; padding: 0px; overflow: auto;">
        ...
      </div>
    </div>
  </div>
</div>

<div id="dialog-image-layout" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="dialog-image-signature" aria-hidden="true">
  <div class="modal-dialog" role="document" style="width: 100%; height: calc(100% -  60px); padding: 40px;">
    <div class="modal-content" style="height: 100%;background: #222">
      <div class="modal-body" style="height: 100%; padding: 0px; overflow: auto;">
        <div class="layout-preview">
            #foreach($temp in $layout-list)
                <div id="type-${temp.name}" class="layout-view">
                    <img src="/tiny/resources/images/ocr/layout/type-${temp.name}.jpg" />
                    <p>${temp.value}</p>
                </div>
            #end
            <div id="type-unknown" class="layout-view">
                <div class="layout-na">NA</div>
                <p>Unknown Layout</p>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>


<div id="dialog-recon-detail" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="dialog-recon-detail" aria-hidden="true">
  <div class="modal-dialog" role="document" style="width: 100%; height: calc(100% -  60px); padding: 0px;">
    <div class="modal-content" style="height: 100%; background: #222">
      <div class="modal-body" style="height: 100%; padding: 0px; overflow: hidden;">
        <div class="recon-detail">
        </div>
        <div class="splitter-horizontal"></div>
        <div class="recon-image">
            <div id="preview-recon-position" class="preview-recon-position">
                <div>
                    <label style="padding-left: 10px; width: 80px">Fixed-Rect</label>
                    <span>W:</span><input type="text" id="recon-box-w" name="recon-box-w" disabled/>
                    <span>H:</span><input type="text" id="recon-box-h" name="recon-box-h" disabled/>
                    <span>X:</span><input type="text" id="recon-box-x" name="recon-box-x" disabled/>
                    <span>Y:</span><input type="text" id="recon-box-y" name="recon-box-y" disabled/>
                    <label id="clean-btn" class="action"
                        style="display: none; margin-left: 15px; padding-left: 2px; padding-right: 2px; background: #555; color: #fff; border-radius: 2px; line-height: 18px; cursor: pointer;"
                        onclick="cleanRectangle()" tooltip="CTRL + ALT + C" flow="up">
                        <i class="fa fa-eraser" aria-hidden="true"></i>
                    </label>
                    <label class="action" id="sign-canvas-switch-pre" style="margin-left: 15px; cursor: pointer; display: none; ">
                        <i class="fa fa-toggle-off"></i>
                    </label>
                    <label class="action" id="sign-image-cut-pre"
                        style="margin-left: 15px; opacity: 0.6; color: #222; width: 20px; text-align: center; border-radius: 2px; padding-right: 5px;"
                        onclick="preCanvasSwitchClick(this)" tooltip="Fix Mark Area" flow="up">
                        <i class="fa fa-scissors" aria-hidden="true"></i>
                    </label>
                </div>
            </div>
            <div class="preview-ct">
            </div>
        </div>
      </div>
    </div>
  </div>
</div>